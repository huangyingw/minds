[remote "origin"]
    url = git@gitlab.com:minds/minds.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/minds.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
    message = n
[vrun]
    silent = false
    async = false
[deploy]
    host = mini
    path = ~/myproject/git/ossn/Minds/minds
[gdif]
    ignore = ./Minds-Install.sh
    ignore = ./var/log/containner.log
    ignore = .gitmodules
    ignore = composer-setup.php
    ignore = engine
    ignore = files.rev
    ignore = front
    ignore = init.sh
    ignore = run.rsh
    ignore = run.sh
    ignore = sockets
