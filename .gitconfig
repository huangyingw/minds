[remote "origin"]
    url = git@github.com:Minds/minds.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/minds.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
    message = n
[vrun]
    silent = false
    async = false
[deploy]
    host = server
    path = ~/myproject/git/ossn/Minds/minds
[gdif]
    ignore = composer-setup.php
    ignore = files.rev
    ignore = run.rsh
